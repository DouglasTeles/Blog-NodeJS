{{#each postagens}}
<div class="card mt-4"> 
    <div class="card-body">
        
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>

        <a href="/postagem/{{slug}}"><button class="btn btn-outline-info">Saiba mais</button></a>
        <hr>
        <br>
        <small>Data de pubicação: {{data}}</small>
    </div>
</div>

{{else}}
    <h5>Nenhuma postagem</h5>
{{/each}}